  <body>
    <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#nav"
        aria-controls="nav"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <a class="navbar-brand" href="#">QA Evaluator</a>

      <div class="collapse navbar-collapse" id="nav">
        <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
          <li class="nav-item active">
            <a class="nav-link" href="#"
              >Experiment
              <span class="sr-only">(current)</span>
            </a>
          </li>
          <li>
            <a class="nav-link" href="/compare">Compare results </a>
          </li>
        </ul>
      </div>
    </nav>

    <div class="container-fluid">
      <div class="row">
        <form id="exForm" class="col-md-12" onsubmit="return false;">
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="exName">Experiment Name</label>
              <input
                type="text"
                class="form-control"
                id="exName"
                placeholder="insert an experiment name"
                minlength="3"
                maxlength="25"
                value="TestEx"
                required
              />
            </div>
            <div class="form-group col-md-6">
              <label for="exDataset">Dataset</label>
              <select class="form-control" id="exDataset">
              </select>
            </div>
            <div class="form-group col-md-3">
              <label for="exQASystems"
                >QA Systems
                <i
                  class="material-icons pressable"
                  data-toggle="modal"
                  data-target="#qanaryPipelines"
                >
                  info
                </i>
              </label>
              <select class="form-control" id="exQASystems">
                <option value="false">-my own-</option>
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20EarlRelationLinking,%20ClsNliodCls,%20SINA/"
                  >Qanary 1</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20EarlRelationLinking,%20ClsNliodCls,%20QueryBuilder/"
                  >Qanary 2</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20EarlRelationLinking,%20DiambiguationClass,%20SINA/"
                  >Qanary 3</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20EarlRelationLinking,%20DiambiguationClass,%20QueryBuilder/"
                  >Qanary 4</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20RelNliodRel,%20ClsNliodCls,%20SINA/"
                  >Qanary 5</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20RelNliodRel,%20ClsNliodCls,%20QueryBuilder/"
                  >Qanary 6</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20RelNliodRel,%20DiambiguationClass,%20SINA/"
                  >Qanary 7</option
                >
                <option
                  value="gerbil-execute/NERD-DBpediaSpotlight,%20RelNliodRel,%20DiambiguationClass,%20QueryBuilder/"
                  >Qanary 8</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20EarlRelationLinking,%20ClsNliodCls,%20SINA/"
                  >Qanary 9</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20EarlRelationLinking,%20ClsNliodCls,%20QueryBuilder/"
                  >Qanary 10</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20EarlRelationLinking,%20DiambiguationClass,%20SINA/"
                  >Qanary 11</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20EarlRelationLinking,%20DiambiguationClass,%20QueryBuilder/"
                  >Qanary 12</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20RelNliodRel,%20ClsNliodCls,%20SINA/"
                  >Qanary 13</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20RelNliodRel,%20ClsNliodCls,%20QueryBuilder/"
                  >Qanary 14</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20RelNliodRel,%20DiambiguationClass,%20SINA/"
                  >Qanary 15</option
                >
                <option
                  value="gerbil-execute/TagmeNED,%20RelNliodRel,%20DiambiguationClass,%20QueryBuilder/"
                  >Qanary 16</option
                >
                <option
                  value="gerbil-execute/NER-Stanford,%20NED-AGDISTIS,%20EarlRelationLinking,%20ClsNliodCls,%20SINA/"
                  >Qanary 17</option
                >
                <option
                  value="gerbil-execute/NER-Stanford,%20NED-AGDISTIS,%20EarlRelationLinking,%20ClsNliodCls,%20QueryBuilder/"
                  >Qanary 18</option
                >
                <option
                  value="gerbil-execute/NER-Stanford,%20NED-AGDISTIS,%20EarlRelationLinking,%20DiambiguationClass,%20SINA/"
                  >Qanary 19</option
                >
                <option
                  value="gerbil-execute/NER-Stanford,%20NED-AGDISTIS,%20EarlRelationLinking,%20DiambiguationClass,%20QueryBuilder/"
                  >Qanary 20</option
                >
                <option
                  value="gerbil-execute/NER-Stanford,%20NED-AGDISTIS,%20RelNliodRel,%20ClsNliodCls,%20SINA/"
